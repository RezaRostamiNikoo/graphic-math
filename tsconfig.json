{
    "include": [
        "./src/**/*.ts",
    ],
    "compilerOptions": {
        "esModuleInterop": true,
        "allowJs": true,
        "outDir": "dist",
        "declaration": true,
        "declarationDir": "dist/typings",
        "declarationMap": true,
        "forceConsistentCasingInFileNames": true,
    },
    "exclude": [
        "node_modules"
    ]
}